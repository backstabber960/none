local FEPlayers, PlayerBtns = {}, {}
local KillSec = FETab:CollapsingHeader({ Title = PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({7875,11025,11340,11340,3360,8400,11340,10185,12705,10605,11970}) })

local function getPlayersList()
    FEPlayers = {}
    for _,plr in ipairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer then table.insert(FEPlayers, plr.Name) end
    end
end

local function clearPlayerButtons()
    for _,b in ipairs(PlayerBtns) do pcall(function() b:Destroy() end) end
    PlayerBtns = {}
end

local function killByName(name)
    siminf()
    local m = Workspace:FindFirstChild(getgenv().Settings.ModelName)
    if not m then return end
    local tp = Players:FindFirstChild(name)
    if not tp or not tp.Character then return end
    local tHRP = tp.Character:FindFirstChild(PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
    if not tHRP then return end
    local nHRP = m:FindFirstChild(PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180})) or m:FindFirstChildWhichIsA(PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({6930,10185,12075,10605,8400,10185,11970,12180}))
    if not nHRP then return end

    local oldAlign = nHRP:FindFirstChild(PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({6825,11340,11025,10815,11550,8400,11655,12075,11025,12180,11025,11655,11550}))
    if oldAlign then oldAlign:Destroy() end

    local a0 = nHRP:FindFirstChild(PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({6825,11340,11025,10815,11550,6825,12180,12180,10185,10395,10920,5040})) or Instance.new(PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({6825,12180,12180,10185,10395,10920,11445,10605,11550,12180}), nHRP)
    a0.Name = PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({6825,11340,11025,10815,11550,6825,12180,12180,10185,10395,10920,5040})
    local a1 = tHRP:FindFirstChild(PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({6825,11340,11025,10815,11550,6825,12180,12180,10185,10395,10920,5145})) or Instance.new(PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({6825,12180,12180,10185,10395,10920,11445,10605,11550,12180}), tHRP)
    a1.Name = PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({6825,11340,11025,10815,11550,6825,12180,12180,10185,10395,10920,5145})

    local al = Instance.new(PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({6825,11340,11025,10815,11550,8400,11655,12075,11025,12180,11025,11655,11550}), nHRP)
    al.Name = PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({6825,11340,11025,10815,11550,8400,11655,12075,11025,12180,11025,11655,11550})
    al.MaxForce = 1e9
    al.Responsiveness = 50
    al.RigidityEnabled = true
    al.Attachment0 = a0
    al.Attachment1 = a1
    al.ApplyAtCenterOfMass = true

    local bv = nHRP:FindFirstChild(PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({6930,11655,10500,12705,9030,10605,11340,11655,10395,11025,12180,12705}))
    if bv then bv:Destroy() end
    bv = Instance.new(PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({6930,11655,10500,12705,9030,10605,11340,11655,10395,11025,12180,12705}), nHRP)
    bv.MaxForce = Vector3.new(1e9,1e9,1e9)
    bv.Velocity = Vector3.new(0,0,0)
end

local function rebuildKillList()
    clearPlayerButtons()
    for _,name in ipairs(FEPlayers) do
        local btn = KillSec:Button({ Text = name, Callback = function() killByName(name) end })
        table.insert(PlayerBtns, btn)
    end
end

FETab:Button({
    Text = PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({8610,10605,10710,11970,10605,12075,10920,3360,8400,11340,10185,12705,10605,11970,3360,7875,11025,11340,11340}),
    Callback = function()
        getPlayersList()
        rebuildKillList()
    end
})

task.spawn(function()
    while true do
        local d = getgenv().Settings.AutoTPDistance
        local s = getgenv().Settings.SavedPos
        if d and s then
            local c = LocalPlayer.Character
            if c and c.Parent then
                local hrp = c:FindFirstChild(PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
                local m = Workspace:FindFirstChild(getgenv().Settings.ModelName)
                if hrp and m then
                    local mp
                    for _,p in ipairs(m:GetChildren()) do if p:IsA(PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({6930,10185,12075,10605,8400,10185,11970,12180})) then mp=p break end end
                    if mp then
                        if (hrp.Position - mp.Position).Magnitude <= d then
                            hrp.CFrame = s
                            task.wait(1)
                        end
                    end
                end
            end
        end
        task.wait(0.5)
    end
end)    
