local function siminf()
    pcall(function() sethiddenproperty(LocalPlayer,PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({8715,11025,11445,12285,11340,10185,12180,11025,11655,11550,8610,10185,10500,11025,12285,12075}),math.huge) end)
    pcall(function() LocalPlayer.SimulationRadius = math.huge end)
end

local espBox
local function createESP(part)
    if espBox then espBox:Destroy() end
    espBox = Instance.new(PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({6930,11655,12600,7560,10185,11550,10500,11340,10605,6825,10500,11655,11970,11550,11445,10605,11550,12180}), part)
    espBox.Name = PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({7245,8715,8400})
    espBox.Adornee = part
    espBox.AlwaysOnTop = true
    espBox.ZIndex = 10
    espBox.Size = part.Size
    espBox.Color3 = getgenv().Settings.ESPColor
    espBox.Transparency = 0.5
end

local function removeESP()
    if espBox then espBox:Destroy() espBox=nil end
end

local espLoop
Tab:Checkbox({
    Label = PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({10605,12075,11760}),
    Value = false,
    Callback = function(_, state)
        getgenv().Settings.ESPEnabled = state
        if state then
            espLoop = task.spawn(function()
                while getgenv().Settings.ESPEnabled do
                    local m = Workspace:FindFirstChild(getgenv().Settings.ModelName)
                    if m then
                        for _,p in ipairs(m:GetChildren()) do
                            if p:IsA(PWyYKnNvPHNcmZmksoRugagErFblcGxFWxetPW({6930,10185,12075,10605,8400,10185,11970,12180})) then createESP(p) break end
                        end
                    else
                        removeESP()
                    end
                    task.wait(1)
                end
                removeESP()
            end)
        else
            removeESP()
        end
    end
})

